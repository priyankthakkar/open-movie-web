{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"jest": true,
		"browser": true,
		"node": true
	},
	"plugins": [
		"prettier"
	],
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react"
	]
}